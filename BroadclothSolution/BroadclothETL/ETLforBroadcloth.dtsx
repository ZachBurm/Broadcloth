                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
	
	DimCompliance.Compliance_SK,
	DimCustomer.Customer_SK,
	DimShipment.Shipment_SK,
	DimProductionBatch.Production_SK,
	DimDate.Date_SK,
	Broadcloth.dbo.ProductionBatch.QuantityProduced AS Quantity_Produced,
	Broadcloth.dbo.ProductionBatch.QualityRating AS Quality_Rating,
	Broadcloth.dbo.ProductionBatch.ProductionCost AS Production_Cost,
	CAST((Broadcloth.dbo.ProductionBatch.ActualEndTime - Broadcloth.dbo.ProductionBatch.StartDateTime) AS INT) AS Production_Duration
FROM Broadcloth.dbo.Customer
INNER JOIN Broadcloth.dbo.Shipment
	ON Broadcloth.dbo.Customer.CustomerID = Broadcloth.dbo.Shipment.CustomerID
INNER JOIN Broadcloth.dbo.Factory
	ON Broadcloth.dbo.Shipment.FactoryID = Broadcloth.dbo.Factory.FactoryID
INNER JOIN Broadcloth.dbo.Compliance
	ON Broadcloth.dbo.Compliance.FactoryID = Broadcloth.dbo.Factory.FactoryID
INNER JOIN Broadcloth.dbo.ProductionBatch
	ON Broadcloth.dbo.Factory.FactoryID = Broadcloth.dbo.ProductionBatch.FactoryID



INNER JOIN DimCustomer
	ON DimCustomer.Customer_AK = Broadcloth.dbo.Customer.CustomerID
INNER JOIN DimCompliance
	ON DimCompliance.Compliance_AK = Broadcloth.dbo.Compliance.ComplianceID
INNER JOIN DimDate
	ON  DimDate.Date = Broadcloth.dbo.ProductionBatch.StartDateTime
INNER JOIN DimShipment
	ON  DimShipment.Shipment_AK = Broadcloth.dbo.Shipment.ShipmentID
INNER JOIN DimProductionBatch
	ON CONCAT(DimProductionBatch.FactoryID, DimProductionBatch.ItemID, DimProductionBatch.Start_date_time) = CONCAT(Broadcloth.dbo.ProductionBatch.FactoryID, Broadcloth.dbo.ProductionBatch.ItemID, Broadcloth.dbo.ProductionBatch.StartDateTime)</property>
